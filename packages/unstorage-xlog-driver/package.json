{
  "name": "@web.worker/unstorage-xlog-driver",
  "version": "0.2.0",
  "description": "unjs/unstorage driver for xlog.app",
  "repository": "Otto-J/unstorage-xlog-deriver",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build:dts": "tsc --emitDeclarationOnly --project tsconfig.type.json",
    "build:source": "bun build ./src/index.ts --outfile=dist/index.mjs  --target node --external unstorage --minify",
    "build": "bun run build:source && bun run build:dts",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "bun run build",
    "release": "bun test && changelogen --release && npm publish && git push --follow-tags",
    "test": "tsc --noEmit --skipLibCheck &&bun test"
  },
  "devDependencies": {
    "changelogen": "^0.5.5"
  },
  "dependencies": {
    "crossbell": "^1.9.0",
    "gray-matter": "^4.0.3"
  }
}
