{
  "name": "@web.worker/unstorage-xlog-driver",
  "type": "module",
  "version": "0.3.0",
  "description": "unjs/unstorage driver for xlog.app",
  "license": "MIT",
  "repository": "Otto-J/unstorage-xlog-deriver",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": "./dist/types.d.ts"
  },
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "rm -rf dist",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepack": "pnpm build",
    "test": "bun test",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "crossbell": "^1.9.0",
    "gray-matter": "^4.0.3",
    "unstorage": "^1.15.0",
    "ofetch": "^1.4.1",
    "zod": "^3.24.3",
    "pino": "^9.6.0"
  },
  "devDependencies": {
    "tsdown": "^0.12.9",
    "typescript": "^5.8.3",
    "changelogen": "^0.5.5"
  }
}
